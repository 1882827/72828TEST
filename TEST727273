tabteleport:AddButton("Safe Spot", function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

    local posX, posY, posZ = 100000, -200, 100000

    -- Criar plataforma
    local platform = Instance.new("Part")
    platform.Size = Vector3.new(50, 1, 50) -- tamanho da plataforma
    platform.Position = Vector3.new(posX, posY - 3, posZ) -- um pouco abaixo do jogador
    platform.Anchored = true
    platform.Color = Color3.fromRGB(255, 0, 0) -- cor vermelha
    platform.Transparency = 0 -- visível
    platform.Parent = workspace

    -- Teleportar jogador
    humanoidRootPart.CFrame = CFrame.new(posX, posY, posZ)

    -- Notificação
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Teletransporte",
        Text = "Teleported with Safe Platform",
        Duration = 2
    })
end)
